    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "markdownlint-cli2": "^0.17.2",
    "obsidian": "^1.8.7",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
